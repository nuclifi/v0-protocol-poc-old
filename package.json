{
  "name": "@nucli.fi/v0-protocol-poc",
  "version": "1.0.0",
  "description": "POC smart contracts for the Nucli.fi V0 protocol.",
  "main": "-",
  "repository": "git@github.com:nuclifi/v0-protocol-poc.git",
  "author": "Nucli.fi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "remove:cache": "rm -rf cache",
    "remove:artifacts": "rm -rf artifacts",
    "remove:typechain-types": "rm -rf typechain-types",
    "remove:all": "npm run remove:artifacts && npm run remove:cache && npm run remove:typechain-types",
    "clean": "npm run remove:all && hardhat clean",
    "solhint:check": "solhint **/*.sol",
    "pretty-quick:all": "pretty-quick --staged --pattern 'contracts/**/*.sol' --pattern 'scripts/**/*.ts' --pattern 'test/**/*.ts'",
    "pre-commit": "npm run pretty-quick:all && npm run solhint:check",
    "compile": "npm run clean && hardhat compile"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.1.1",
    "@commitlint/config-conventional": "^17.1.0",
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@openzeppelin/contracts": "^4.7.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.2.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.4.7",
    "hardhat": "^2.10.2",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "pretty-quick": "^3.1.3",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-node": ">=8.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "contributors": [
    "Yash Agrawal <yashagrawal2299@gmail.com>"
  ],
  "keywords": [
    "nucli.fi",
    "protocol",
    "poc",
    "smart-contracts"
  ]
}
